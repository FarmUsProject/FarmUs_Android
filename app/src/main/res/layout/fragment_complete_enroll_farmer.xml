<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <ImageView
        android:id="@+id/iv_round_check_box"
        android:layout_width="86dp"
        android:layout_height="86dp"
        android:src="@drawable/round_check_box"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="204dp"
        app:layout_constraintHorizontal_bias="0.5"/>

    <TextView
        android:id="@+id/tv_complete_enroll_farmer"
        android:fontFamily="@font/line_seed_kr_bd"
        android:textSize="22sp"
        android:textColor="@color/text_1"
        android:layout_marginTop="17dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="농장주 등록 완료!"
        app:layout_constraintTop_toBottomOf="@id/iv_round_check_box"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        style="@style/Body2Reg"
        android:textColor="@color/text_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이제 나의 농지를 등록하고 분양할 수 있어요."
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@id/tv_complete_enroll_farmer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/btn_navigate_home"
        style="@style/Body1Med"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="홈으로 이동"
        android:textColor="@color/white"
        android:background="@color/main"
        android:gravity="center"
        android:paddingVertical="14dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginBottom="67dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>